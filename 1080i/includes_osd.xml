<?xml version="1.0" encoding="UTF-8"?>
<includes>
  <include name="OSD_MediaFlags">
    <!-- Resolution Flag -->
    <control type="image">
      <width>140</width>
      <height>95</height>
      <bordersize>2</bordersize>
      <texture fallback="flags/colour/140x95/resolution/defaultscreen.png">$VAR[OSDResolutionFlag_140x95]</texture>
      <include>MediaFlagVars</include>
    </control>
    <!-- Video Codec Flag -->
    <control type="image">
      <width>140</width>
      <height>95</height>
      <bordersize>2</bordersize>
      <texture fallback="flags/colour/140x95/video/default.png">$VAR[OSDVideoCodecFlag_140x95]</texture>
      <include>MediaFlagVars</include>
    </control>
    <!-- Aspect Ratio Flag -->
