  <variable name="Weather_FanartSettingVar">
    <value condition="String.IsEqual(Skin.String(Weather.Fanart),Slideshow)">Skin.SetString(Weather.Fanart,Faded)</value>
    <value condition="String.IsEqual(Skin.String(Weather.Fanart),Faded)">Skin.SetString(Weather.Fanart,Prairie)</value>
    <value condition="String.IsEqual(Skin.String(Weather.Fanart),Prairie)">Skin.Reset(Weather.Fanart)</value>
    <value>Skin.SetString(Weather.Fanart,Slideshow)</value>
