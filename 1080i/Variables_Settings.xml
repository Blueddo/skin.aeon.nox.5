  <!-- Weather Fanart Label -->
  <variable name="Weather_FanartLabelVar">
    <value condition="String.IsEqual(Skin.String(Weather.Fanart),Slideshow)">$LOCALIZE[42454]</value>
    <!-- STRING: Slideshow -->
    <value condition="String.IsEqual(Skin.String(Weather.Fanart),Faded)">$LOCALIZE[42455]</value>
    <!-- STRING: Faded -->
    <value condition="String.IsEqual(Skin.String(Weather.Fanart),Prairie)">$LOCALIZE[42456]</value>
    <!-- STRING: Faded -->
    <value>$LOCALIZE[42453]</value>
